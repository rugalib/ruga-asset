{
  "name": "rugalib/ruga-asset",
  "description": "Asset library for the ruga framework",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "composer-runtime-api": "^2.1",
    "psr/container": "^1.1",
    "laminas/laminas-view": "^2.20",
    "laminas/laminas-json": "^3.3",
    "rugalib/ruga-log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-config-aggregator": "^1.7",
    "laminas/laminas-servicemanager": "^3.10"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Asset\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Asset\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Asset",
      "config-provider": "Ruga\\Asset\\ConfigProvider"
    }
  }
}